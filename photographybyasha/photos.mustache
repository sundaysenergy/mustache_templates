<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Photography By Asha</title>

    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0-rc1/css/bootstrap.min.css" media="screen">
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
  </head>
<!-- compile url:  -->
  <body style="background-color:black;">
    <div class="container">
      <h1>Photography By Asha</h1>

      <div class="row">

        <div id="carousel-generic" class="carousel slide">
          <!-- Wrapper for slides -->
          <div class="carousel-inner" style="text-align:center;height:600px;">
            {{#contents}}
            <div class="item {{class}}" style="height:100%;">
              <img src="{{img_resize_host}}/1000x600{{path}}" alt="{{alt}}" style="position:absolute;margin:auto;top:0;bottom:0;left:0;right:0;">
              <div class="carousel-caption">
              </div>
            </div>
            {{/contents}}
          </div>

          <!-- Controls -->
          <a class="left carousel-control" href="#carousel-generic" data-slide="prev">
            <span class="icon-prev"></span>
          </a>
          <a class="right carousel-control" href="#carousel-generic" data-slide="next">
            <span class="icon-next"></span>
          </a>
        </div>

      </div> <!-- end example -->

    </div>

    <!-- JavaScript plugins (requires jQuery) -->
    <script src="http://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0-rc1/js/bootstrap.min.js"></script>
    <script type="text/javascript">
    $('document').ready(function() {
      $('#carousel-generic').carousel({
        interval: 7000,
        wrap: true
      });
      // $( "#carousel-generic .carousel-inner .item" ).each(function( index ) {
      //    $(this).css("margin-top",-(($(this).height())/4) + 'px');
      // });
      $('img').on("contextmenu mousedown",function(e){
          e.preventDefault();
          return false;
      });
    });
    </script>
  </body>
</html>
